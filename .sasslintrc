{
	"rules": {
		"extends-before-mixins": 1,
		"extends-before-declarations": 1,
		"placeholder-in-extend": 0,
		"mixins-before-declarations": 1,
		"one-declaration-per-line": 1,
		"empty-line-between-blocks": 0,
		"single-line-per-selector": 0,
		"no-attribute-selectors": 0,
		"no-color-hex": 0,
		"no-color-keywords": 0,
		"no-color-literals": [1, {
			"allow-rgba": true,
			"allow-map-identifiers": false,
			"allow-variable-identifiers": false
		}],
		"no-combinators": 0,
		"no-css-comments": 0,
		"no-debug": 1,
		"no-disallowed-properties": 0,
		"no-duplicate-properties": 1,
		"no-empty-rulesets": 2,
		"no-extends": 0,
		"no-ids": 0,
		"no-important": 0,
		"no-invalid-hex": 2,
		"no-mergeable-selectors": 2,
		"no-misspelled-properties": 2,
		"no-qualifying-elements": [1, {
			"allow-element-with-attribute": true,
			"allow-element-with-class": true
		}],
		"no-trailing-whitespace": 1,
		"no-trailing-zero": 1,
		"no-transition-all": 0,
		"no-universal-selectors": 0,
		"no-url-domains": 1,
		"no-url-protocols": 1,
		"no-vendor-prefixes": 1,
		"no-warn": 1,
		"property-units": 0,
		"declarations-before-nesting": 1,
		"force-attribute-nesting": 0,
		"force-element-nesting": 0,
		"force-pseudo-nesting": 0,
		"class-name-format": 1,
		"function-name-format": 1,
		"id-name-format": 0,
		"mixin-name-format": 1,
		"placeholder-name-format": 1,
		"variable-name-format": 1,
		"attribute-quotes": 1,
		"bem-depth": 0,
		"border-zero": [1, { "convention": "none" }],
		"brace-style": 1,
		"clean-import-paths": 1,
		"empty-args": 0,
		"hex-length": [1, { "style": "long" }],
		"hex-notation": 1,
		"indentation": [1, { "size": "tab" }],
		"leading-zero": [1, { "include": true }],
		"max-line-length": 0,
		"max-file-line-count": 0,
		"nesting-depth": [1, { "max-depth": 3 }],
		"property-sort-order": 0,
		"pseudo-element": 1,
		"quotes": 1,
		"shorthand-values": [1, { "allowed-shorthands": [1, 2] }],
		"url-quotes": 1,
		"variable-for-property": 1,
		"zero-unit": 1,
		"space-after-comma": 1,
		"space-before-colon": 1,
		"space-after-colon": 1,
		"space-before-brace": 1,
		"space-before-bang": 1,
		"space-after-bang": 1,
		"space-between-parens": 1,
		"space-around-operator": 1,
		"trailing-semicolon": 1,
		"final-newline": 1
	}
}
